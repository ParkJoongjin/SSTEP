<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/staffInvite5_list_dialog2_L1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <!-- 승인 대기중 내용_frame -->
    <FrameLayout
        android:id="@+id/staffInvite3_F5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="5dp">
        <!-- 사람이미지 -->
        <ImageView
            android:id="@+id/staffInvite3_pimage2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/yimage_female" />

        <!-- 이름 -->
        <TextView
            android:id="@+id/staffInvite3_nametv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="5dp"
            android:text="이름"
            android:textColor="#737373"
            android:textSize="13sp" />

        <!-- 아이디 -->
        <TextView
            android:id="@+id/staffInvite3_idtv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="25dp"
            android:text="아이디"
            android:textColor="@color/black"
            android:textSize="13sp" />

        <!-- 재전송 버튼 -->
        <TextView
            android:id="@+id/staffInvite3_yesbtn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="15dp"
            android:layout_marginRight="70dp"
            android:text="재전송"
            android:textColor="@color/blue"
            android:textSize="13sp" />

        <!-- 삭제 버튼 -->
        <TextView
            android:id="@+id/staffInvite3_nobtn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="15dp"
            android:layout_marginRight="25dp"
            android:text="삭제"
            android:textColor="#737373"
            android:textSize="13sp" />
    </FrameLayout> <!-- 승인대기중 내용 리스트 frame -->
</LinearLayout>